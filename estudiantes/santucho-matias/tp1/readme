sobreescribir imagen

docker build -t generador_imagen . 

correr el contenedor  generador

docker run -it -v /home/matias/edp-2023-ci/estudiantes/santucho-matias/tp1/generador/outputs_generador:/app/outputs generador_imagen 

servidor ftp 

docker run -p 21:21 -p 21000-21010:21000-21010 -e USERS="tuiaedpuser|tuiaedpuser" -v ./generador/outputs_generador/:/ftp/tuiaedpuser delfer/alpine-ftp-server

lista archivos serv ftp 

curl -L ftp://tuiaedpuser:tuiaedpuser@0.0.0.0/ 
